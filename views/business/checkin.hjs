<div style="height: 50px; width: 130px;  position:absolute; top:420px; left:540px;">
    <input type="button" class="button" data-toggle="modal" data-target=".bs-example-modal-lg"value="Check in">

    <!-- Large modal -->
    <!--<button class="btn btn-primary" data-toggle="modal" data-target=".bs-example-modal-lg">Large modal</button>-->

    <div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-body">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title" id="myModalLabel">Checked in!</h4>
                    </div>
                    <div class="modal-body">
                        <p>Thank you checking in!</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script type="text/javascript">
        $('[data-toggle=popover]').popover();
    </script>
</div>

