    <style>
        .button {
            padding: 12px 20px 45px 20px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 16px;
            margin: 4px 2px;
            -webkit-transition-duration: 0.4s; /* Safari */
            transition-duration: 0.4s;
            cursor: pointer;
        }


        .button {
            background-color: #c0c0c0;
            color: black;
            border: 2px solid #555555;
        }

        .button:hover {
            background-color: #555555;
            color: white;
        }
